      <Separator layoutX="30.0" layoutY="36.0" prefHeight="22.0" prefWidth="542.0" />
      <Label layoutX="30.0" layoutY="14.0" text="Planos do sistema">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button layoutX="531.0" layoutY="12.0" mnemonicParsing="false" onAction="#btnVoltarPressed" text="Voltar" />
      <Button layoutX="30.0" layoutY="495.0" mnemonicParsing="false" onAction="#btnCadastrarPressed" text="Cadastrar novo" />
      <ComboBox fx:id="selectPlano" layoutX="30.0" layoutY="58.0" prefWidth="150.0" promptText="Escolher plano" />
