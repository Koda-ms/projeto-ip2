            <TableColumn fx:id="dtFimColuna" prefWidth="75.0" text="Data fim" />
            <TableColumn fx:id="objetivoColuna" prefWidth="75.0" text="Objetivo" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
